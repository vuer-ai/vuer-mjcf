<mujoco model="teddy_bear">
    <option solver="CG" tolerance="1e-6" timestep=".001" integrator="implicitfast"/>
    <size memory="100M"/>

  <asset>
    <!-- point at your original OBJ (not the tetrahedral msh) -->
    <mesh name="teddy_visual" file="low-poly_teddy_bear.obj__sf.obj"/>
  </asset>
  
  <worldbody>
    <!-- <body name="debug" pos="0 0 0">
      <geom type="mesh" mesh="teddy_visual" rgba="1 1 1 0.5"/>
    </body> -->
<!-- 
    <flexcomp type="mesh" file="low-poly_teddy_bear.obj" pos="0 0 .1" dim="2" euler="90 0 0"
              radius=".001" rgba="0 .7 .7 1" mass=".05" name="softbody">
      <elasticity young="1e3" poisson="0.1" damping="0.001"/>
      <contact selfcollide="none" internal="false"/>
    </flexcomp> -->

    <body name="gmsh" pos="0 0 0.3" quat="0.7071 0.7071 0.0 0.0">
        <flexcomp name="gmsh" type="gmsh" file="low-poly_teddy_bear.obj_.msh" radius="0.005" dim="2" scale="0.05 0.05 0.05">
            <!-- <contact internal="false" selfcollide="none" /> -->
            <contact condim="1" selfcollide="none" solimp=".95 .99 .0001" solref="0.01 1"></contact>
            <edge equality="true" />
        </flexcomp>
    </body>

    <body name="table" pos="0 0 0">
      <geom type="box" size="1 1 .01" rgba=".8 .8 .8 1"/>
      </body>

  </worldbody>
</mujoco>